import java.util.Scanner;
public class Exercici08 {

	public static void main(String[] args) {
		// Intercanviar els valors de dues variables prèviament introduïdes
		Scanner e = new Scanner(System.in);
		public string var1 , var2 ;
		
		System.out.print("Variable 1 : ");var1 = e.next();		
		System.out.print("Variable 2 : ");var2 = e.next();
		
		var1,var2 = var2,var1;
		
		System.out.println("Variable 1 : "+var1+"\nVariable 2 : "+var2);
		
		
	}

}
